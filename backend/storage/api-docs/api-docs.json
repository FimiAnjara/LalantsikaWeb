        "/notifications/send": {
            "post": {
                "tags": [
                    "Notifications"
                ],
                "summary": "Envoyer une notification push à un utilisateur",
                "description": "Envoyer une notification à un utilisateur",
                "operationId": "088e9c35c886513afc6d109728d82295",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "id_utilisateur",
                                    "title",
                                    "body"
                                ],
                                "properties": {
                                    "id_utilisateur": {
                                        "description": "ID de l'utilisateur",
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "title": {
                                        "description": "Titre de la notification",
                                        "type": "string",
                                        "example": "Nouveau message"
                                    },
                                    "body": {
                                        "description": "Corps de la notification",
                                        "type": "string",
                                        "example": "Vous avez reçu un nouveau message"
                                    },
                                    "data": {
                                        "description": "Données supplémentaires",
                                        "type": "object",
                                        "example": {
                                            "type": "message",
                                            "id": "123"
                                        }
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Notification envoyée avec succès",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Notification sent successfully"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
